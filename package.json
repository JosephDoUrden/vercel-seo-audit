{
  "name": "vercel-seo-audit",
  "version": "0.5.1",
  "description": "CLI tool that diagnoses SEO and indexing issues for Next.js/Vercel websites",
  "type": "module",
  "bin": {
    "vercel-seo-audit": "./bin/vercel-seo-audit.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node bin/vercel-seo-audit.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "seo",
    "audit",
    "vercel",
    "nextjs",
    "cli"
  ],
  "author": "JosephDoUrden",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDoUrden/vercel-seo-audit.git"
  },
  "homepage": "https://github.com/JosephDoUrden/vercel-seo-audit#readme",
  "bugs": {
    "url": "https://github.com/JosephDoUrden/vercel-seo-audit/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "fast-xml-parser": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
